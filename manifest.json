{
    "name": "Sabnzbd+ Status",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Check your Sabnzbd+ queue status",
    "icons": {
        "16": "icons/mg16x16.png",
        "32": "icons/mg32x32.png",
        "48": "icons/mg48x48.png",
        "128": "icons/mg128x128.png"
    },
    "browser_action": {
        "default_popup": "popup/popup.html"
    },
    "background": {
        "scripts": ["background/background.js"],
        "persistent": false
    }
}